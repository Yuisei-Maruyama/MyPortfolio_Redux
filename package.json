{
  "name": "portfolio_next_redux",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-commit": "eslint --fix './src/**/*.{js,ts,tsx}' && eslint --fix './tools/**/*.{js,ts,tsx}' && prettier --write './src/**/*.{js,ts,tsx}' && prettier --write './tools/**/*.{js,ts,tsx}'",
    "lint-fix": "npm run lint-commit && npx ts-node tools/convertReadme.ts && git add README.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.1",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/styled-components": "^5.1.25",
    "axios": "^0.27.2",
    "eslint-plugin-styled-components-varname": "^1.0.1",
    "mongodb": "^4.6.0",
    "next": "12.1.6",
    "normalizr": "^3.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^11.1.2",
    "polished": "^4.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npm run lint-fix"
    ]
  }
}
